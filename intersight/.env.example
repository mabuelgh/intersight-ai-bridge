# =============================================================================
# STEP 1: JSON Configuration Template Variables (config.template.json)
# =============================================================================

# Server Configuration
SERVER_SERIAL=your_server_serial_here
SERVER_PROFILE_NAME=IAIB-AI-IMM-1

# VLAN Configuration
NATIVE_VLAN_ID=100
ALLOWED_VLANS=100
INBAND_VLAN_ID=100

# Management Network Configuration
MGMT_IP_FROM=192.168.1.100
MGMT_IP_TO=192.168.1.100
MGMT_GATEWAY=192.168.1.1
MGMT_PRIMARY_DNS=8.8.8.8
MGMT_SECONDARY_DNS=8.8.4.4

# MAC Address Pool
MAC_POOL_NAME=IAIB-MAC-Pool

# =============================================================================
# STEP 2: Ubuntu Config & OS Install Variables
# =============================================================================

# Intersight API Configuration (os_install.py)
INTERSIGHT_HOST=https://intersight.com
INTERSIGHT_API_KEY_ID=your_api_key_id_here
INTERSIGHT_PRIVATE_KEY_PATH=./SecretKey.txt
ORGANIZATION_NAME=default
SERVER_NAME=your_server_name_here

# OS Installation Images (os_install.py)
OS_IMAGE_NAME=ubuntu-24.04.3-live-server-amd64.iso
SCU_IMAGE_NAME=SCU.7.1.4.iso

# Ubuntu Server Network Configuration (ubuntu-ai-config.template)
PROXY_URL=http://your-proxy.company.com:80
SERVER_IP_ADDRESS=192.168.1.100/24
SERVER_GATEWAY=192.168.1.1
SERVER_INTERFACE=eno5
SERVER_HOSTNAME=ai-server
SERVER_TIMEZONE=UTC
PRIMARY_DNS=8.8.8.8